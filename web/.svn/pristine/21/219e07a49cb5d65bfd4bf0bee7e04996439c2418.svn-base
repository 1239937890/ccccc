    <script type="text/javascript">
        $(function(){
            resize1()
            $(window).resize(function(){
                resize1()
            });
            function resize1(){
                $(".tourenter .item .imgbox").height( $(".tourenter .item .imgbox").width()*560/750+"px")
            };
            $("header .navs .item").eq(3).addClass("on");
            $(".details").click(function(){
                var id = $(this).attr('data-id');
                var price = $(this).attr('data-price');
                var title = $(this).attr('data-title');
                var dhour = $(this).attr('data-hour');
                var ddays = $(this).attr('data-ddays');
                var ticket_status = $(this).attr('data-status');
                var valid_date = $(this).attr('data-valid-date');
                var sms_intro = $(this).attr('data-sms-intro');
                var intro = $(this).attr('data-intro');
                var refund_extra = $(this).attr('data-refund-extra');
                var fee = $(this).attr('data-fee');
                var cert = $(this).attr('data-cert');
                var fee_un = $(this).attr('data-fee-un');
                var ex_time = $(this).attr('data-ex-time');
                var uunotes = $(this).attr('data-notes');
                if(ddays == 0){
                    ddays = '日期的'
                }else{
                    ddays = '前'+ddays+'天的'
                };
                if(valid_date == 0){
                    valid_date = '游玩日期起有效'
                }else if(valid_date == 1){
                    valid_date = '下单日期起有效'
                }else{
                    valid_date = '期票模式'
                };
                var uplimit = $(this).attr('data-up-limit');
                var addr = $(this).attr('data-addr');
                var refund_rule = $(this).attr('data-refund-rule');
                var rule ='';
                if(refund_rule == 0){
                    rule = '游玩日期内可退'/*1游玩日期前可退需在验证
                    截止时间前退，2 不可退*/
                }else if(refund_rule == 1){
                    rule = '游玩日期前可退需在验证截止时间前退';
                }else if(refund_rule == 2){
                    rule = '不可退'
                };
                //console.log(price)
                $('.ticket-box .num').text('¥'+price);
                $('.ticket-box .title').text(title);
                $('.ticket-box .dhour').text(dhour);
                $('.ticket-box .uplimit').text(uplimit);
                $('.ticket-box .addr').text(addr);
                $('.ticket-box .refund_rule').text(rule);
                $('.ticket-box .ddays').text(ddays);
                $('.ticket-box .ex_time').text(ex_time);
                $('.ticket-box .valid_date').text(valid_date);
                $('.ticket-box .sms_intro').text(sms_intro);
                $('.ticket-box .intro').text(intro);
                $('.ticket-box .fee').text(fee);
                $('.ticket-box .cert').text(cert);
                $('.ticket-box .fee_un').text(fee_un);
                $('.ticket-box .refund_extra').text(refund_extra);
                $('.ticket-box .uunotes').text(uunotes);
                $(".mask").fadeIn();
                $(".ticket-box").fadeIn();
                if(price > 0){
                    if(ticket_status == 0){
                        $('#buy_submit').removeAttr('onclick');
                        $('#buy_submit').attr('href','/g/flow/'+id);
                    }
                }
            });
            $(".ticket-close").click(function(){
                $(".mask").fadeOut()
                $(".ticket-box").fadeOut()
            })
            var swiper = new Swiper('.news1-top .swiper-container', {
                speed:800,
                autoplay: {
                    delay: 5000,
                },
                slidesPerView: 2,
                breakpoints: {
                    767: {
                        slidesPerView: 1
                    },
                }
            });
        });
    </script>

